#!/bin/bash

# Bed brugeren om at indtaste et tal
echo -n "Indtast et tal: "
read tal

# Kontroller, at inputtet er et tal
if ! [[ "$tal" =~ ^[0-9]+$ ]]; then
    echo "Fejl: Du skal indtaste et positivt heltal."
    exit 1
fi

# Brug en for-løkke til at tælle fra 1 til det indtastede tal
for (( i=1; i<=tal; i++ ))
do
    echo $i
done
